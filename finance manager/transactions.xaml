<Window x:Class="finance_manager.transactions"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:finance_manager"
        mc:Ignorable="d"
        Title="transactions" Height="450" Width="800">
  <Grid Background="#FF191A1F">
    <Button x:Name="backbtn" Content="back" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="85" Height="32" Background="#FF2F2F2F" Foreground="White" FontWeight="Bold" Click="backbtn_Click"/>
    <StackPanel Margin="64,142,81,0" Background="#FF6D6D6D">
      <StackPanel Height="49" Background="#FF868686"/>
    </StackPanel>
    <Label Content="Youre last transactions" HorizontalAlignment="Center" Margin="0,116,0,0" VerticalAlignment="Top" Foreground="White" FontWeight="Bold"/>
    <Button x:Name="addnewtransactionbtn" Content="Add new transaction" HorizontalAlignment="Left" Margin="583,109,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.177,-0.599" Height="28" Width="136" Background="#FF3A3A3A" FontWeight="Bold" Foreground="White" Click="addnewtransactionbtn_Click"/>

  </Grid>
</Window>
